       FILE-CONTROL.
           SELECT M-CUENTAS
           ASSIGN TO "\COBOL\arch\mcuentas-parcial.idx"
           ORGANIZATION INDEXED
                           ACCESS DYNAMIC
                           RECORD KEY CTAS-CLAVE
                 ALTERNATE RECORD KEY CTAS-CLAVE-1
                           WITH DUPLICATES
                 ALTERNATE RECORD KEY CTAS-CLAVE-2 = CTAS-PROVINCIA
                           WITH DUPLICATES
