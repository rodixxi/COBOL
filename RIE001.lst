           SELECT LISTADO
           ASSIGN TO "\COBOL\listado\LISTADO-RIE001"
           ORGANIZATION IS SEQUENTIAL.
       COPY "\COBOL\fuentes\cpy\plasticos.sel".
           SELECT M-PLASTICOS ASSIGN TO DISK
                              "\COBOL\arch\plasticos.idx"
                              ORGANIZATION IS INDEXED
