      *----------------------------------------------------------------
       PROCEDURE DIVISION.

       CONTROL-PROG.
000005     PERFORM INICIO     THRU F-INICIO
000009     PERFORM PROCESO    THRU F-PROCESO
00000D     PERFORM FINAL-PROG THRU F-FINAL-PROG
000011     GOBACK.

       INICIO.
00001B     PERFORM ABRIR-ARCHIVO THRU F-ABRIR-ARCHIVO
00001F     ACCEPT WK-FECHA       FROM CENTURY-DATE
000025     CALL "FEC-NAC-ED" USING WK-FECHA
                                   WK-FECHA-ED-1
                                   WK-FECHA-ED-2
                                   WK-FECHA-ED-3
00003C     MOVE WK-FECHA-ED-1    TO TIT-TITULO-FECHA
000043     PERFORM ENCABEZAR     THRU F-ENCABEZAR.
       F-INICIO. EXIT.

       ABRIR-ARCHIVO.
000050     OPEN INPUT MOV-CTAS
00005B     OPEN OUTPUT LISTADO.
       F-ABRIR-ARCHIVO. EXIT.

       ENCABEZAR.
00006F     ADD 1 TO TIT-TITULO-HOJA
      * IMPRIME ENCABEZADO
000075     IF TIT-TITULO-HOJA = 1
00007F        WRITE REGISTRO FROM TIT-TITULO AFTER 0
00009A     ELSE
00009B        WRITE REGISTRO FROM TIT-TITULO AFTER PAGE
           END-IF
0000B0     WRITE REGISTRO FROM TIT-LINEA
0000C2     WRITE REGISTRO FROM TIT-DETALLE

\Cobol\fuentes\CTAS001.cbl Thu Dec 03 23:35:23 2015  ACUCOBOL-GT v7.2.0  Page: 0004


0000D5     WRITE REGISTRO FROM TIT-LINEA
0000E7     MOVE 4 TO WK-LINEA.
       F-ENCABEZAR. EXIT.

       PROCESO.
0000F6     INITIALIZE WK-MOV-LEIDOS
0000FB     PERFORM UNTIL WK-FINAL = 1
000105        READ MOV-CTAS AT END
000111                         MOVE 1 TO WK-FINAL
000117                         EXIT PERFORM CYCLE
              END-READ
00011B        ADD 1 TO WK-MOV-LEIDOS
000121        INITIALIZE LIS-REG
000143        EVALUATE M-MOV
00014C           WHEN "A" PERFORM PROCESO-A THRU F-PROCESO-A
00015B           WHEN "B" PERFORM PROCESO-B THRU F-PROCESO-B
00016A           WHEN "M" PERFORM PROCESO-M THRU F-PROCESO-M
000171           WHEN OTHER PERFORM PROCESO-OTROS THRU F-PROCESO-OTROS
              END-EVALUATE
           END-PERFORM.
       F-PROCESO. EXIT.

       PROCESO-OTROS.
000181     PERFORM DETALLE     THRU F-DETALLE
000185     MOVE TAB-LEYENDA(1) TO L-OBS
00018C     WRITE REGISTRO      FROM LIS-REG
00019E     ADD 1 TO WK-ERRORES.
       F-PROCESO-OTROS. EXIT.

       DETALLE.
0001AD     MOVE M-MOV  TO L-MOV
0001B3     MOVE M-CTA  TO L-CTA
0001BA     MOVE M-APE  TO L-APE
0001C1     MOVE M-NOM  TO L-NOM
0001C8     MOVE M-PROV TO L-PROV
0001CE     MOVE M-FNAC TO WK-FECHA
0001D5     CALL "FEC-NAC-ED" USING WK-FECHA
                                   WK-FECHA-ED-1
                                   WK-FECHA-ED-2
                                   WK-FECHA-ED-3
0001EC     MOVE WK-FECHA-ED-2 TO L-FNAC.
       F-DETALLE. EXIT.

       PROCESO-A.
0001FC     IF M-CTA = SPACE
              OR M-FNAC = SPACE
              OR  M-APE = SPACE
              OR M-NOM = SPACE
              OR M-PROV = SPACE
000225        PERFORM DETALLE     THRU F-DETALLE
000229        MOVE TAB-LEYENDA(2) TO L-OBS
000230        WRITE REGISTRO      FROM LIS-REG
000242        ADD 1 TO WK-ERRORES
           ELSE
00024B        ADD 1 TO WK-ALTAS-OK
           END-IF.
       F-PROCESO-A. EXIT.

\Cobol\fuentes\CTAS001.cbl Thu Dec 03 23:35:23 2015  ACUCOBOL-GT v7.2.0  Page: 0005



       PROCESO-M.
00025A     IF M-CTA = SPACE
000263        PERFORM DETALLE     THRU F-DETALLE
000267        MOVE TAB-LEYENDA(3) TO L-OBS
00026E        WRITE REGISTRO      FROM LIS-REG
000280        ADD 1 TO WK-ERRORES
           ELSE
000289        IF M-FNAC = SPACE
                 OR M-APE = SPACE
                 OR M-NOM = SPACE
                 OR M-PROV = SPACE
0002AA           PERFORM DETALLE THRU F-DETALLE
0002AE           MOVE TAB-LEYENDA(4) TO L-OBS
0002B5           WRITE REGISTRO   FROM LIS-REG
0002C7           ADD 1 TO WK-ERRORES
              ELSE
0002D0           ADD 1 TO WK-MODIF-OK
              END-IF
           END-IF.
       F-PROCESO-M. EXIT.

       PROCESO-B.
0002DF     IF M-CTA = SPACE
0002E8        PERFORM DETALLE THRU F-DETALLE
0002EC        MOVE TAB-LEYENDA(5) TO L-OBS
0002F3        WRITE REGISTRO   FROM LIS-REG
000305        ADD 1 TO WK-ERRORES
           ELSE
00030E        ADD 1 TO WK-BAJAS-OK
           END-IF.
       F-PROCESO-B. EXIT.

       FINAL-PROG.
00031D     PERFORM TOTALES           THRU F-TOTALES
000321     PERFORM CERRAR-ARCHIVO    THRU F-CERRAR-ARCHIVO.
       F-FINAL-PROG. EXIT.

       TOTALES.
      * IMPRIME PIE DE PAGINA CON TOTAL DE ALUMNOS
00032E     IF WK-LINEA > 60
000338        PERFORM ENCABEZAR
           END-IF
00033C     MOVE WK-MOV-LEIDOS TO TIT-LEIDOS-NUM
000342     MOVE WK-ALTAS-OK   TO TIT-ALTAS-NUM
000348     MOVE WK-BAJAS-OK   TO TIT-BAJAS-NUM
00034E     MOVE WK-MODIF-OK   TO TIT-MODIF-NUM
000354     MOVE WK-ERRORES    TO TIT-ERRORES-NUM
00035A     WRITE REGISTRO FROM TIT-LINEA
00036C     WRITE REGISTRO FROM TIT-LEIDOS
00037F     WRITE REGISTRO FROM TIT-ALTAS
000392     WRITE REGISTRO FROM TIT-BAJAS
0003A5     WRITE REGISTRO FROM TIT-MODIF
0003B8     WRITE REGISTRO FROM TIT-ERRORES
0003CB     IF WK-LINEA > 63
0003D5        PERFORM ENCABEZAR
           END-IF

\Cobol\fuentes\CTAS001.cbl Thu Dec 03 23:35:23 2015  ACUCOBOL-GT v7.2.0  Page: 0006


0003D9     PERFORM VERIFICAR-TOTALES THRU F-VERIFICAR-TOTALES.
       F-TOTALES. EXIT.

       VERIFICAR-TOTALES.
0003E6     COMPUTE WK-TOTALES = WK-ALTAS-OK +
                                WK-BAJAS-OK +
                                WK-MODIF-OK +
                                WK-ERRORES
0003F6     IF WK-TOTALES <> WK-MOV-LEIDOS
000400        WRITE REGISTRO FROM "TOTALES NO BALANCEAN"
           END-IF.
       F-VERIFICAR-TOTALES. EXIT.

       CERRAR-ARCHIVO.
00041C     CLOSE MOV-CTAS
                 LISTADO.
       F-CERRAR-ARCHIVO. EXIT.

       END PROGRAM.

